{
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "rules": {
    "no-console": false,
    "no-reference": false,
    "prettier": true,
    "variable-name": [
      true,
      "allow-leading-underscore"
    ]
  },
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ]
}
